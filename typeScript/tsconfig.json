{
	"compilerOptions": {
		"rootDir": "./",
		"outDir": "../js",
		"target": "es5",
		"forceConsistentCasingInFileNames": true,
		"lib": [
			"dom",
			"es5",
			"scripthost",
			"es2015.promise"
		],
		"noFallthroughCasesInSwitch": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"strict": true,
		"sourceMap": true,
		"strictBindCallApply": false,
		"strictNullChecks": false
	},
	"include": [
		"./**/*"
	]
}